{
  "name": "complex-expression-parser",
  "version": "2.0.0",
  "description": "Parses and evaluates expressions over the complex numbers",
  "main": "es5/expression.js",
  "scripts": {
    "prepare": "npm run build & npm test & npm run prepare-web",
    "test": "jasmine",
    "prepare-web": "browserify -r ./build/src/expression.js:expression -r ./build/src/complex.js:complex -r ./build/src/complex-math.js:complex-math -o ./es5/expression.js",
    "build-src": "babel ./es6 --out-dir ./build/src",
    "build-test": "babel ./spec --out-dir ./build/test",
    "build": "npm run build-src & npm run build-test"
  },
  "keywords": [
    "math",
    "maths",
    "mathematics",
    "complex",
    "evaluate",
    "expression",
    "parse",
    "parsing"
  ],
  "author": "Colin Jeanne <colinjeanne@hotmail.com> (http://www.colinjeanne.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colinjeanne/expression-parser.git"
  },
  "devDependencies": {
    "babel": "^5.2.6",
    "browserify": "^11.0.0",
    "jasmine": "^2.3.1"
  }
}
